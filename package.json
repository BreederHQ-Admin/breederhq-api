{
  "name": "breederhq-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "start:render": "bash -lc \"DATABASE_URL=$MIGRATE_DATABASE_URL npx prisma migrate deploy && npm run start\"",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.dev.migrate -- prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.prod.migrate -- prisma migrate deploy",
    "prisma:status:dev": "dotenv -e .env.dev.migrate -- prisma migrate status",
    "prisma:status:prod": "dotenv -e .env.prod.migrate -- prisma migrate status",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^13.0.1",
    "@prisma/client": "^6.16.1",
    "fastify": "^4.28.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "dotenv-cli": "^7.4.2",
    "prisma": "^6.16.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2"
  }
}